{
    "name": "node-red-contrib-boolean-parser",
    "version": "1.3.1",
    "description": "A Node-RED node to parse and convert several types of on/off, start/stop, true/false values.",
    "homepage": "https://github.com/SimonButtazzi/node-red-contrib-boolean-parser#readme",
    "bugs": {
        "url": "https://github.com/SimonButtazzi/node-red-contrib-boolean-parser/issues"
    },
    "license": "MIT",
    "author": {
        "name": "Simon Buttazzi",
        "email": "simon.buttazzi@gmail.com",
        "url": "https://www.buttazzi.de"
    },
    "node-red": {
        "version": ">=3.0.0",
        "nodes": {
            "bool": "boolean-parser.js"
        }
    },
    "engines": {
        "node": ">=12.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SimonButtazzi/node-red-contrib-boolean-parser.git"
    },
    "keywords": [
        "node-red",
        "parser",
        "boolean",
        "bool",
        "translator",
        "converter",
        "cast",
        "on-off"
    ],
    "main": "boolean-parser.js",
    "auto-changelog": {
        "template": "keepachangelog",
        "unreleased": false,
        "commitLimit": false,
        "ignoreCommitPattern": "^\\[RELEASE\\]",
        "tagPattern": "^v[0-9]+\\.[0-9\\.]+$",
        "package": true,
        "prepend": true
    },
    "scripts": {
        "preversion": "",
        "version": "auto-changelog && git add CHANGELOG.md",
        "postversion": "cp CHANGELOG.md test123.md"
    }
}
